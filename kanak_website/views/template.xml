    <template id="ContactUs_snippets" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_content']//t[@t-snippet][last()]" position="after">
            <t t-snippet="kanak_website.contact_us_form" t-thumbnail="/kanak_website/static/src/img/contactus_form.png" />
