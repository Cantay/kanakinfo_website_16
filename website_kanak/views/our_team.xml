                <section class="pt32 pb32">
                    <div class="container">
                        <!-- <h2>Carousel Example</h2>   -->
                        <div id="TeamCarousel" class="carousel slide" data-ride="carousel" data-interval="2000">
                            <!-- Indicators -->
                            <ol class="carousel-indicators">
                                <li data-bs-target="#TeamCarousel" data-bs-slide-to="0" class="active"></li>
                                <li data-bs-target="#TeamCarousel" data-bs-slide-to="1"></li>
                                <li data-bs-target="#TeamCarousel" data-bs-slide-to="2"></li>
                            </ol>
                            <!-- Wrapper for slides -->
                            <div class="carousel-inner">
                                <div class="carousel-item active">
                                    <img src="/website_kanak/static/src/img/our-team/slider1.webp" alt="Los Angeles" style="width:100%;" />
                                </div>
                                <div class="carousel-item">
                                    <img src="/website_kanak/static/src/img/our-team/slider2.webp" alt="Chicago" style="width:100%;" />
                                </div>
                                <div class="carousel-item">
                                    <img src="/website_kanak/static/src/img/our-team/slider3.webp" alt="New york" style="width:100%;" />
                                </div>
                            </div>
                            <!-- Left and right controls -->
                            <a class="left carousel-control" data-bs-target="#TeamCarousel" data-bs-slide="prev">
                                <span class="glyphicon glyphicon-chevron-left"></span>
                                <span class="sr-only">Previous</span>
                            </a>
                            <a class="right carousel-control" data-bs-target="#TeamCarousel" data-bs-slide="next">
                                <span class="glyphicon glyphicon-chevron-right"></span>
                                <span class="sr-only">Next</span>
                            </a>
                        </div>
                    </div>
                </section>
                
            </div>
