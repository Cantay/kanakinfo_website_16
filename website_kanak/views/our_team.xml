<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="our_team_page" name="our team">
        <t t-call="website.layout">
            <t t-set="pageName" t-value="'OurTeam kanak_our_team'"/>
            <div id="wrap" class="our_team_kanak">
                <section class="our-team-banner pt32 pb32">
                    <div class="container">
                        <div class="row align-items-center justify-content-center">
                            <div class="col-lg-6">
                                <h1 style="font-size: 46pt;" class="text-center">Our Musketeers</h1>
                                <p class="text-center red" style="font-size: 18pt;">In Unity, There is Strength</p>
